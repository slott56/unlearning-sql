{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "activation_source.schema.json",
  "title": "Activation Source",
  "description": "The source activation records from some other application",
  "type": "object",
  "properties": {
    "customer_name": {"type": "string", "minLength": 1, "maxlength": 64},
    "device_name": {"type": "string", "minLength": 1, "maxlength": 16},
    "service_name": {"type": "string", "minLength": 1, "maxlength": 16},
    "start_date": {"type": "string", "maxlength": 64, "format": "date-time"},
    "latitude": {"type": "string", "maxlength": 16, "format": "latitude"},
    "longitude": {"type": "string", "maxlength": 16, "format": "longitude"}
  }
}
